    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-sslify": "^1.2.0",
    "framer-motion": "^4.1.17",
    "jest": "^27.4.5",
    "leasot": "^12.0.0",
    "morgan": "^1.10.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.1.1",
    "react-spotify-web-playback-sdk": "^2.0.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "vite-plugin-environment": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "tslint": "^5.12.1",
    "typescript": "^4.3.2",
    "vite": "^2.6.4",
    "vite-plugin-pwa": "^0.11.12"
  },
  "homepage": "https://github.com/akuakuka/sptfyplayer",
  "main": "dist/index.js",
  "name": "sptfyplayer",
  "engines": {
    "node": "17.x"
  },
  "scripts": {
    "frontend": "export NODE_ENV=development && vite --host 0.0.0.0",
    "backend": "export NODE_ENV=development && ts-node-dev --respawn  --project server/tsconfig.json ./server/index.ts",
    "dev": "npx concurrently \"npm run frontend\" \"npm run backend\"",
    "start1": "tsc && node ./server/dist/index.js",
    "start": "npm run build && cd ./server && tsc && node --max_old_space_size=2560 dist/index.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll ",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "todo": "leasot './**/*.ts' './**/*.tsx' --ignore './node_modules/'",
    "startdev": "export NODE_ENV=production && npm run build && cd ./server && tsc && node --max_old_space_size=2560 dist/index.js"
  }
}
